Caso 2:
Clasicas = 0

Moto = int(input("\nIndique la fecha de fabricacion de la primera motocicleta:"))
if Moto <= 1992:
    Clasicas+=1 
Moto = int(input("\nIndique la fecha de fabricacion de la segunda motocicleta:"))
if Moto <= 1992:
    Clasicas+=1 
Moto = int(input("\nIndique la fecha de fabricacion de la tercera motocicleta:"))
if Moto <= 1992:
    Clasicas+=1
Moto = int(input("\nIndique la fecha de fabricacion de la cuarta motocicleta:"))
if Moto <= 1992:
    Clasicas+=1
Moto = int(input("\nIndique la fecha de fabricacion de la quinta motocicleta:"))
if Moto <= 1992:
    Clasicas+=1
Moto = int(input("\nIndique la fecha de fabricacion de la sexta motocicleta:"))
if Moto <= 1992:
    Clasicas+=1
Moto = int(input("\nIndique la fecha de fabricacion de la septima motocicleta:"))
if Moto <= 1992:
    Clasicas+=1
Moto = int(input("\nIndique la fecha de fabricacion de la octava motocicleta:"))
if Moto <= 1992:
    Clasicas+=1
Moto = int(input("\nIndique la fecha de fabricacion de la penultima motocicleta:"))
if Moto <= 1992:
    Clasicas+=1
Moto = int(input("\nIndique la fecha de fabricacion de la ultima motocicleta:"))
if Moto <= 1992:
    Clasicas+=1

print("Numero de motocicletas clasicas:", Clasicas)

Caso 3:

print("\n")
Salario = int(input("\nIndique el salario del usuario 1:"))
if Salario < 500:
    Dinero = abs (Salario - 500) ;
    print("\n")
    print("El dinero que se necesita: ", Dinero, "dolares");
if Salario >= 500:
    print("\n")
    print("El salario cumple con lo establecido")

